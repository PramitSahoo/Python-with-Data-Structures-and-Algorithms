    "n = int(input())\n",
    "i = 1\n",
    "while(i <= n):\n",
    "    j = 1\n",
    "    ch = chr(ord('A') + n - 1)\n",
    "    ch_p = chr(ord(ch)- i + 1)\n",
    "    while(j <= i):\n",
    "        alphabet_pattern = chr(ord(ch_p) +j-1)\n",
    "        print(alphabet_pattern,end='')\n",
    "        j += 1\n",
    "    print()\n",
    "    i += 1"
   ]
