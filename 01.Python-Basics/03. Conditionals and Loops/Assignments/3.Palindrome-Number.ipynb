   "source": [
    "num = int(input())\n",
    "# palindrome means num and reverse of that num should be same\n",
    "temp = num\n",
    "revNum = 0\n",
    "while(temp > 0):\n",
    "    rem = temp % 10\n",
    "    temp = temp // 10\n",
    "    revNum = revNum * 10 + rem\n",
    "if(num == revNum):\n",
    "    print(\"true\")\n",
    "else:\n",
    "    print(\"false\")"
   ]
