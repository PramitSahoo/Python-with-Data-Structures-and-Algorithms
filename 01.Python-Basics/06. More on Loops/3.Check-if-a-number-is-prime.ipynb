    "n = int(input())\n",
    "isDivided = False\n",
    "\n",
    "for d in range(2,n,1):\n",
    "    if(n%d == 0):\n",
    "        isDivided = True\n",
    "        #break\n",
    "if(isDivided):\n",
    "    print(\"Not Prime\")\n",
    "else:\n",
    "    print(\"Prime\")\n",
    "    "
