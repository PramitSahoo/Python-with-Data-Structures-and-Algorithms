    "n = int(input())\n",
    "r = int(input())\n",
    "\n",
    "# ncr using function\n",
    "def ncr(n):\n",
    "    fact = 1\n",
    "    for i in range(1, n+1):\n",
    "        fact = fact * i\n",
    "    return fact\n",
    "\n",
    "# ncr \n",
    "NcR = ncr(n)//(ncr(r)*ncr(n-r))\n",
    "print(NcR)"
