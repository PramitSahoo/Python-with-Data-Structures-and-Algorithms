   "source": [
    "def countZeros(n):\n",
    "    count = 0\n",
    "    if (n < 10):\n",
    "        if(n == 0):\n",
    "            return 1\n",
    "        else:\n",
    "            return 0\n",
    "    smallAns = countZeros(n /10)\n",
    "    if(smallAns % 10  == 0):\n",
    "        count = count + 1\n",
    "    return count"
   ]
